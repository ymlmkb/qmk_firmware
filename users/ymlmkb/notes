# ---------------------------------------------------------------------------------------------------------------------------------

cdqmk
rm -f *.bin *.hex; gmake clean all:ymlmkb

# ---------------------------------------------------------------------------------------------------------------------------------

vi keyboards/tada68/keymaps/ymlmkb/keymap.c \
   keyboards/kbdfans/kbd67/rev2/keymaps/ymlmkb/keymap.c \
   keyboards/massdrop/alt/keymaps/ymlmkb/keymap.c \
   keyboards/fc660c/keymaps/ymlmkb/keymap.c \
   keyboards/dztech/dz60rgb/keymaps/ymlmkb/keymap.c \
   keyboards/nk65/keymaps/ymlmkb/keymap.c

vi keyboards/nullbitsco/scramble/keymaps/ymlmkb/keymap.c

vi keyboards/ymlmkb/wasdv2_teensy/keymaps/ymlmkb/keymap.c keyboards/ymlmkb/ortho_tetris/keymaps/ymlmkb/keymap.c

vi users/ymlmkb/ymlmkb.c users/ymlmkb/ymlmkb.h

# ---------------------------------------------------------------------------------------------------------------------------------

TADA68 (Tada68)

vi keyboards/tada68/keymaps/ymlmkb/keymap.c
gmake tada68:ymlmkb
sleep 10 && cat tada68_ymlmkb.bin > "/Volumes/TADA68  /FLASH.BIN"
# hurry up and reset the keyboard

# ---------------------------------------------------------------------------------------------------------------------------------

KBDFans KBD67v2

vi keyboards/kbdfans/kbd67/rev2/keymaps/ymlmkb/keymap.c
gmake kbdfans/kbd67/rev2:ymlmkb
gmake kbdfans/kbd67/rev2:ymlmkb:flash
# reset the keyboard at your leisure; the flasher will check every 5 seconds

# ---------------------------------------------------------------------------------------------------------------------------------

Leopold FC660C (Hasu)

vi keyboards/fc660c/keymaps/ymlmkb/keymap.c
gmake fc660c:ymlmkb
gmake fc660c:ymlmkb:flash
# reset the keyboard at your leisure; the flasher will check every 5 seconds

# ---------------------------------------------------------------------------------------------------------------------------------

Drop Alt (Massdrop)

vi keyboards/massdrop/alt/keymaps/ymlmkb/keymap.c
gmake massdrop/alt:ymlmkb
cd ~/Opt/massdrop-loader
./mdloader_mac --first --download ~/Source/qmk_firmware/massdrop_alt_ymlmkb.bin --restart
# reset the keyboard at your leisure; the flasher will check every second (for a minute, i think)

# ---------------------------------------------------------------------------------------------------------------------------------

DZ60 (ATMega)

vi keyboards/dztech/dz60rgb/keymaps/ymlmkb/keymap.c
gmake dztech/dz60rgb/v2:ymlmkb
gmake dztech/dz60rgb/v2:ymlmkb:flash
# reset the keyboard at your leisure; the flasher will check every 5 seconds

# ---------------------------------------------------------------------------------------------------------------------------------

NK65 (NovelKeys)

vi keyboards/nk65/keymaps/ymlmkb/keymap.c
gmake nk65:ymlmkb
gmake nk65:ymlmkb:flash
# reset the keyboard at your leisure; the flasher will check every 5 seconds

# =================================================================================================================================

Nullbits Scramblepad

vi keyboards/nullbitsco/scramble/keymaps/ymlmkb/keymap.c
gmake nullbitsco/scramble:ymlmkb
# plug in the scramble with the lower-right-hand key held down
gmake nullbitsco/scramble:ymlmkb:flash

# ---------------------------------------------------------------------------------------------------------------------------------

WASDv2_Hacked (Teensy 2.0)

vi keyboards/ymlmkb/wasdv2_teensy/keymaps/ymlmkb/keymap.c
gmake ymlmkb/wasdv2_teensy:ymlmkb
sleep 10 &&
teensy_loader_cli -w --mcu=atmega32u4 -v ymlmkb_wasdv2_teensy_ymlmkb.hex
# reset the keyboard at your leisure; the flasher will wait (indefinitely?)

# ---------------------------------------------------------------------------------------------------------------------------------

ymlmkb_ortho_tetris_ymlmkb.hex
