# ---------------------------------------------------------------------------------------------------------------------------------

TADA68 (Tada68)

vi keyboards/tada68/keymaps/clsmyth/keymap.c

qmk compile -kb tada68 -km clsmyth
# THEN
sleep 10 && cat tada68_clsmyth.bin > "/Volumes/TADA68  /FLASH.BIN"
# OR (may require add'l keyboard for password)
# cat tada68_clsmyth.bin | sudo tee "/Volumes/TADA68  /FLASH.BIN"

# ---------------------------------------------------------------------------------------------------------------------------------

KBDFans KBD67v2

vi keyboards/kbdfans/kbd67/rev2/keymaps/clsmyth/keymap.c

qmk compile -kb kbdfans/kbd67/rev2 -km clsmyth
# THEN
sleep 10 &&
dfu-programmer atmega32u4 erase &&
dfu-programmer atmega32u4 flash kbdfans_kbd67_rev2_clsmyth.hex &&
dfu-programmer atmega32u4 reset

